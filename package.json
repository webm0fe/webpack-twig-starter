    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/plugin-transform-spread": "^7.13.0",
    "@babel/preset-env": "^7.13.15",
    "autoprefixer": "^9.8.0",
    "babel-loader": "^8.2.2",
